{
  "applications": [
    {
      "name": "SwingSet3",
      "icon": "${user.dir}/demo/SwingSet3/icon.png",
      "jreExecutable": "${java.home}/bin/java",
      "javaVersion": "${java.version}",
      "vmArgs": "-Xmx128m -DauthorizedUser=${user}",
      "classPathEntries": [
        "${user.dir}/demo/SwingSet3/SwingSet3.jar",
        "${user.dir}/demo/SwingSet3/lib/*.jar"
      ],
      "homeDir": "demo/SwingSet3/${user}",
      "theme": "Murrine",
      "fontConfig" : {
        "dialog" : "${user.dir}/fonts/Roboto-Regular.ttf",
        "dialoginput" : "${user.dir}/fonts/RobotoMono-Regular.ttf",
        "serif" : "${user.dir}/fonts/RobotoSlab-Regular.ttf"
      },
      "maxClients": 1,
      "sessionMode": "CONTINUE_FOR_BROWSER",
      "swingSessionTimeout": 300,
      "allowStealSession": true,
      "antiAliasText": true,
      "authorization": false,
      "isolatedFs": true,
      "debug": true,
      "authentication": false,
      "directdraw": true,
      "allowDelete": true,
      "allowDownload": true,
      "allowAutoDownload": true,
      "allowUpload": true,
      "uploadMaxSize": 5,
      "allowJsLink": true,
      "mainClass": "com.sun.swingset3.SwingSet3",
      "args": ""
    },
    {
      "name": "NetBeans Platform",
      "icon": "icon.png",
      "jreExecutable": "${java.home}/bin/java",
      "javaVersion": "${java.version}",
      "vmArgs": "-Dorg.netbeans.log.startup=print -DTopSecurityManager.disable=true",
      "classPathEntries": [
        "nbplatform.jar",
        "lib/*.jar"
      ],
      "homeDir": "demo/nbmin",
      "theme": "Murrine",
      "maxClients": 1,
      "sessionMode": "CONTINUE_FOR_BROWSER",
      "swingSessionTimeout": 300,
      "allowStealSession": true,
      "antiAliasText": true,
      "authorization": false,
      "isolatedFs": true,
      "debug": true,
      "authentication": false,
      "directdraw": false,
      "allowDelete": true,
      "allowDownload": true,
      "allowAutoDownload": true,
      "allowUpload": true,
      "uploadMaxSize": 5,
      "allowJsLink": true,
      "mainClass": "org.webswing.demo.NbMain",
      "args": ""
    },
    {
      "name": "NetBeans IDE 8.2",
      "icon": "demo/nbmin/icon.png",
      "jreExecutable": "${java.home}/bin/java",
      "javaVersion": "${java.version}",
      "vmArgs": "-Dnetbeans.user=${user.dir}/tmp/netbeans/${user} -Dnetbeans.home=/usr/local/netbeans-8.2/platform -Dorg.netbeans.log.startup=print -DTopSecurityManager.disable=true -Dnetbeans.dirs=\"nb${path.separator}ergonomics${path.separator}ide${path.separator}extide${path.separator}java${path.separator}apisupport${path.separator}webcommon${path.separator}websvccommon${path.separator}enterprise${path.separator}mobility${path.separator}profiler${path.separator}python${path.separator}php${path.separator}identity${path.separator}harness${path.separator}cnd${path.separator}dlight${path.separator}groovy${path.separator}extra${path.separator}javacard${path.separator}javafx\"",
      "classPathEntries": [
        "${JAVA_HOME}/lib/dt.jar",
        "${JAVA_HOME}/lib/tools.jar",
        "platform/lib/boot.jar",
        "platform/lib/nbplatform.jar",
        "platform/lib/org-openide-util.jar",
        "platform/lib/org-openide-util-lookup.jar",
        "platform/lib/org-openide-modules.jar",
        "platform/lib/org-openide-util-ui.jar"
      ],
      "homeDir": "/usr/local/netbeans-8.2",
      "theme": "Murrine",
      "maxClients": 10,
      "sessionMode": "CONTINUE_FOR_BROWSER",
      "swingSessionTimeout": 300,
      "allowStealSession": true,
      "antiAliasText": true,
      "authorization": false,
      "isolatedFs": false,
      "debug": true,
      "authentication": false,
      "directdraw": true,
      "allowDelete": true,
      "allowDownload": true,
      "allowAutoDownload": true,
      "allowUpload": true,
      "uploadMaxSize": 5,
      "allowJsLink": true,
      "mainClass": "org.webswing.demo.NbIDEMain",
      "args": "--branding platform"
    }
  ],
  "applets": [
    {
      "name": "SwingSet3 applet",
      "icon": "${user.dir}/demo/SwingSet3/icon.png",
      "jreExecutable": "${java.home}/bin/java",
      "javaVersion": "${java.version}",
      "vmArgs": "-Xmx128m -DauthorizedUser=${user}",
      "classPathEntries": [
        "${user.dir}/demo/SwingSet3/SwingSet3.jar",
        "${user.dir}/demo/SwingSet3/lib/*.jar"
      ],
      "homeDir": "demo/SwingSet3/${user}",
      "theme": "Murrine",
      "maxClients": -1,
      "sessionMode": "CONTINUE_FOR_BROWSER",
      "swingSessionTimeout": 100,
      "allowStealSession": true,
      "antiAliasText": true,
      "authorization": false,
      "isolatedFs": true,
      "debug": true,
      "authentication": false,
      "directdraw": false,
      "allowDelete": false,
      "allowDownload": false,
      "allowAutoDownload": true,
      "allowUpload": false,
      "uploadMaxSize": 5,
      "allowJsLink": true,
      "appletClass": "org.webswing.demo.applet.SwingSet3Applet",
      "parameters": {
        "param1": "paramvalue1",
        "param2": "paramvalue2"
      }
    }
  ]
}
